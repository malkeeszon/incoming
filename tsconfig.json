{
  "compilerOptions": {
    "module": "commonjs",
    "target":"ES2015",
    "declaration": true,
    "rootDirs": ["./decorators","./server"],
    "outDir": "./lib",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "suppressImplicitAnyIndexErrors": true,
  }
}